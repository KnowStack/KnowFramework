<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.didiglobal.logi.job.mapper.LogIWorkerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.didiglobal.logi.job.common.po.LogIWorkerPO">
        <id column="id" property="id"/>
        <result column="code" property="setWorkerCode"/>
        <result column="name" property="setWorkName"/>
        <result column="cpu" property="cpu"/>
        <result column="cpu_used" property="cpuUsed"/>
        <result column="memory" property="memory"/>
        <result column="memory_used" property="memoryUsed"/>
        <result column="jvm_heap" property="jvmHeap"/>
        <result column="jvm_heap_used" property="jvmHeapUsed"/>
        <result column="job_num" property="jobNum"/>
        <result column="heartbeat" property="heartbeat"/>
        <result column="node_name" property="nodeName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , code, name, cpu, cpu_used, memory, memory_used, jvm_heap, jvm_heap_used, job_num, heartbeat, node_name
    </sql>

</mapper>
